mixin prize(name, img, url)
  li: a(href="#{url}", title="#{name}", data-toggle="tooltip")
    img(src="/images/sponsors/prizes/#{img}.jpg")

mixin prize-logo(name, img, url)
  li: a(href="#{url}", title="Prize from #{name} (see below)", target="_blank", data-toggle="tooltip")
    img(src="/images/sponsors/prizes/#{img}.jpg")

//- prize mixins (for reuse)
mixin one-per-team
  i.icon-users(title="one per team member")

mixin modulus-beer
  .with-popover
    |  case of beer
    .popover-content(style="display:none")
      |  a case of Cincinnati microbews

mixin goinstant-hoodie
  .with-popover
    | (+ Hoodies)
    .popover-content(style="display:none")
      img(src="/images/sponsors/prizes/goinstant-hoodie.jpg", width="250", height="250")

mixin twilio-book
  strong: a(href="http://twilio.com", target="_blank") Twilio
  |  -&nbsp;
  .with-popover
    a(href="http://www.manning.com/cantelon/", target="_blank")
      | NodeJS in Action</a>
    .popover-content(style="display:none")
      img(src="/images/sponsors/prizes/nodejs-in-action.jpg", height="150")
  |  eBook

